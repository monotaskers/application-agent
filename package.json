{
  "name": "agent-application-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:ui\" \"pnpm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug pnpm run dev",
    "dev:agent": "./scripts/run-agent.sh || scripts/run-agent.bat",
    "dev:ui": "next dev --turbopack",
    "build": "NODE_NO_WARNINGS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write .",
    "format:check": "prettier -c -w .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run test:coverage",
    "validate:schema": "./scripts/validate-schema-sync.sh",
    "test:vercel-build": "./scripts/test-vercel-build.sh",
    "prepare": "husky",
    "install:agent": "./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "pnpm run install:agent || echo 'Agent setup skipped (not required for Vercel builds)'"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.35",
    "@ai-sdk/openai": "^1.3.22",
    "@copilotkit/react-core": "1.9.3",
