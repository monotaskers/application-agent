{
  "mcpServers": {
    "copilotkit": {
      "type": "sse",
      "url": "https://mcp.copilotkit.ai/sse"
    },
    "Neon": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.neon.tech/mcp"]
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    },
    "Sentry": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"],
      "env": {},
      "working_directory": null
    }
  }
}